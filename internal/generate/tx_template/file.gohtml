package code

import (
	"context"
	"database/sql"
	"strings"
	{{- if ne .ExtralImport ""}}
	"{{.ExtralImport}}"
	{{- end}}
)

type {{.GoName}}DAO struct {
	Tx *sql.Tx
}

func (dao *{{.GoName}}DAO) commit() error {
	if err := dao.Tx.Commit(); err != nil {
		dao.Tx.Rollback()
		return err
	}
	return nil
}

func (dao *{{.GoName}}DAO) rollback() error {
	return dao.Tx.Rollback()
}

{{template "insert" .}}
{{template "select" .}}
{{template "update" .}}
{{template "delete" .}}
